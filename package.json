{
  "name": "domain-design",
  "version": "0.1.1",
  "private": true,
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "preferGlobal": true,
  "bin": {
    "gen": "script/generate/bin.js",
    "module": "script/module/bin.js"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "yarn test:unit",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "commit": "cross-env allowGitCommit=true git commit -m ''",
    "//": "发布版本",
    "release": "cross-env noVerifyHook=true standard-version",
    "coverage": "yarn test:unit  --coverage && open-cli coverage/lcov-report/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --config ./.lintstagedrc",
      "prepare-commit-msg": "node ./script/prepare-commit-msg.js"
    }
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-e2e-cypress": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-plugin-typescript": "^3.12.0",
    "@vue/cli-plugin-unit-jest": "^3.12.0",
    "@vue/cli-service": "^3.12.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "conventional-changelog-cli": "^2.0.23",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "dotenv": "^8.1.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "fs-extra": "^8.1.0",
    "handlebars": "^4.4.2",
    "handlebars-generator": "^3.2.3",
    "husky": "^3.0.9",
    "lint-staged": "^9.3.0",
    "minimist": "^1.2.0",
    "open-cli": "^5.0.0",
    "prettier": "^1.18.2",
    "standard-changelog": "^2.0.13",
    "standard-version": "^7.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^23.0.0",
    "typescript": "~3.5.1",
    "vue-template-compiler": "^2.6.10"
  }
}
